# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/kortex
DB_USERNAME=postgres
DB_PASSWORD=your_password_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_minimum_64_characters_long
JWT_EXPIRATION=86400000

# Document Storage
DOCUMENT_STORAGE_PATH=./uploads

# HuggingFace API
HUGGINGFACE_API_TOKEN=your_huggingface_token_here
HUGGINGFACE_API_URL=https://api-inference.huggingface.co/pipeline/feature-extraction
HUGGINGFACE_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Qdrant Vector Database
QDRANT_HOST=localhost
QDRANT_PORT=6334
QDRANT_API_KEY=
QDRANT_USE_TLS=false

# LLM Provider (openai or groq)
LLM_PROVIDER=groq

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo

# Groq Configuration
GROQ_API_KEY=your_groq_api_key_here
GROQ_API_URL=https://api.groq.com/openai/v1
GROQ_MODEL=llama3-8b-8192
